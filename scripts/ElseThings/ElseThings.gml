function ElseThings()
{
	if dash = 0 and grapplingHook = 0
	{
		forceMove -= 0.05
		moveSpeed = defaultMoveSpeed
		
		if forceMove <= 0
		{
			forceMove = 0
		}
	}
		
	healTimer = healTimerValue
	
	image_speed = 1
	heal = 0
	
	space = false
	right = false
	left = false
	mouse1 = false
	mouse2 = false
	buttonHeal = false
	buttonHook = false
	buttonDash = false
	
}